package com.mx.pokemonapi.vo;

import java.util.ArrayList;

public class HeldVO {
    private HeldItem item;
    private ArrayList<VersionDetailVO> version_details;

    public HeldVO() {
    }

    public HeldVO(HeldItem item, ArrayList<VersionDetailVO> version_details) {
        this.item = item;
        this.version_details = version_details;
    }

    public HeldItem getItem() {
        return item;
    }

    public void setItem(HeldItem item) {
        this.item = item;
    }

    public ArrayList<VersionDetailVO> getVersionDetails() {
        return version_details;
    }

    public void setVersionDetails(ArrayList<VersionDetailVO> version_details) {
        this.version_details = version_details;
    }

    @Override
    public String toString() {
        return "Held{" +
                "item=" + item +
                ", version_details=" + version_details +
                '}';
    }
}
